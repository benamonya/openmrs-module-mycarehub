<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openmrs.module.mycarehub.model">
    <class name="MyCareHubSetting" table="mycarehub_setting">
        <id name="id" type="java.lang.Integer" column="mycarehub_settings_id" unsaved-value="0">
            <generator class="native"/>
        </id>

        <property name="settingType" type="java.lang.String" not-null="true" column="setting_type" length="255"/>
        <property name="lastSyncTime" type="java.util.Date" not-null="true" column="last_sync_time" length="19"/>

        <property name="dateCreated" type="java.util.Date" column="date_created" length="19" />
        <property name="dateChanged" type="java.util.Date" column="date_changed" length="19" />
        <property name="voided" type="java.lang.Boolean" column="retired" length="1"/>
        <property name="dateVoided" type="java.util.Date" column="date_retired" length="19" />
        <property name="voidReason" type="java.lang.String" column="retire_reason" length="255" />

        <many-to-one name="creator" class="org.openmrs.User"/>
        <many-to-one name="voidedBy" class="org.openmrs.User" column="retired_by" />
        <many-to-one name="changedBy" class="org.openmrs.User" column="changed_by" />
        <property name="uuid" type="java.lang.String" column="uuid" length="38" unique="true" />
    </class>
</hibernate-mapping>
